<%=form_with model: @employee do |f|%>
    First Name: <%=f.text_field :first_name %><br>
    Last Name: <%=f.text_field :last_name %><br>
    Alias: <%=f.text_field :alias %><br>
    Title: <%=f.text_field :title %><br>
    Office: <%=f.text_field :office %><br>
    Dog: <%= f.collection_select(:dog_id, @dogs, :id, :name)%><br>
    Image: <%=f.text_field :img_url%>
    <%=f.submit%>
<%end%>

<% if @employee.errors.any? %>
    <%@employee.errors.full_messages do |errors|%>
        <li><%=puts errors%></li>
    <%end%>
<%end%>

